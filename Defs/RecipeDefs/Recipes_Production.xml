<Defs>

	<RecipeDef Name="MakeStoneBlocksBulkBase" Abstract="True">
		<defName>MakeStoneBlocksBulkBase</defName>
		<jobString>Cutting stone blocks in bulk.</jobString>
		<workAmount>4080</workAmount>
		<effectWorking>CutStone</effectWorking>
		<soundWorking>Recipe_MakeStoneBlocks</soundWorking>
		<targetCountAdjustment>20</targetCountAdjustment>
		<recipeUsers>
			<li>TableStonecutter</li>
		</recipeUsers>
	</RecipeDef>
	
	<RecipeDef ParentName="MakeStoneBlocksBulkBase">
		<defName>MakeStoneBlocksBulkAny</defName>
		<label>make any stone blocks (x3)</label>
		<description>Cuts rock chunks into usable stone blocks in bulk.</description>
		<workerCounterClass>RecipeWorkerCounter_MakeStoneBlocks</workerCounterClass>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>StoneChunks</li>
					</categories>
				</filter>
				<count>3</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>StoneChunks</li>
			</categories>
		</fixedIngredientFilter>
		<specialProducts>
			<li>Butchery</li>
		</specialProducts>
	</RecipeDef>
	
